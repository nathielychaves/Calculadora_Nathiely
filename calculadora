n1= Number(prompt ("Digite um numero:"));
n2= Number(prompt ("Digite um numero:"));

function somar(n1, n2){
  alert("A soma é " + (n1 + n2));
}
somar(n1, n2);

function divisao(n1, n2){
  alert("A divisão é " + (n1 / n2));
}
divisao(n1, n2);

function multiplicacao(n1, n2){
  alert("A multiplicação é " + (n1 * n2));
}
multiplicacao(n1, n2);

function subitracao(n1, n2){
  alert("A subitração é " + (n1 - n2));
}
subitracao(n1, n2);

function resto(n1, n2){
  alert("A resto é " + (n1 % n2));
}
resto(n1, n2);
